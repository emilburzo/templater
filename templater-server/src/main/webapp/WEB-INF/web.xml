<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1">>

    <!-- Servlets -->
    <servlet>
        <servlet-name>appServlet</servlet-name>
        <servlet-class>com.emilburzo.templater.rpc.AppServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>appServlet</servlet-name>
        <url-pattern>/templater/rpc</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>
